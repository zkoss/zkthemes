.z-fisheye,
.z-fisheye-text,
.z-fisheye-img {
	position: absolute;
	cursor: pointer;
}
.z-fisheye {
	z-index: 2;
}
.z-fisheye-img {
	border: 0px; width: 100%; height: 100%;
}
.z-fisheye-text {
	text-align: center; 
	display: none;
	font-family: ${fontFamilyC};
	font-weight: normal;
	font-size: 12px;
	background-color: #FFFFFF; 
	border: 1px solid #CFCFCF;
	padding: 2px 5px;
}
.z-fisheyebar-inner {
	position: relative;
}
